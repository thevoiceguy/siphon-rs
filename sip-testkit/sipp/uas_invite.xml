<?xml version="1.0" encoding="UTF-8"?>
<scenario name="UAS INVITE receiver">
  <!-- Receive INVITE -->
  <recv request="INVITE"/>

  <!-- Send 100 Trying -->
  <send>
<![CDATA[
SIP/2.0 100 Trying
[last_Via:]
[last_From:]
[last_To:];tag=[call_number]
[last_Call-ID:]
[last_CSeq:]
Content-Length: 0

]]>
  </send>

  <!-- Optional: Send 180 Ringing -->
  <pause milliseconds="100"/>
  <send>
<![CDATA[
SIP/2.0 180 Ringing
[last_Via:]
[last_From:]
[last_To:];tag=[call_number]
[last_Call-ID:]
[last_CSeq:]
Contact: <sip:uas@[local_ip]:[local_port]>
Content-Length: 0

]]>
  </send>

  <!-- Send 200 OK -->
  <pause milliseconds="200"/>
  <send>
<![CDATA[
SIP/2.0 200 OK
[last_Via:]
[last_From:]
[last_To:];tag=[call_number]
[last_Call-ID:]
[last_CSeq:]
Contact: <sip:uas@[local_ip]:[local_port]>
Content-Type: application/sdp
Content-Length: [len]

v=0
o=uas 53655765 2353687637 IN IP4 [local_ip]
s=-
c=IN IP4 [local_ip]
t=0 0
m=audio 9002 RTP/AVP 0
a=rtpmap:0 PCMU/8000
]]>
  </send>

  <!-- Receive ACK -->
  <recv request="ACK"/>

  <!-- Wait for BYE or timeout -->
  <recv request="BYE" timeout="30000"/>

  <!-- Send 200 OK for BYE -->
  <send>
<![CDATA[
SIP/2.0 200 OK
[last_Via:]
[last_From:]
[last_To:]
[last_Call-ID:]
[last_CSeq:]
Content-Length: 0

]]>
  </send>
</scenario>
